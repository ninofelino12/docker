<odoo>
  <data>
    <record id="cetakqweb" model="ir.actions.act_window">
      <field name="name">My Custom View</field>
      <field name="res_model">barcodescaner.barcodescaner</field>
      <field name="view_mode">form</field>
      <field name="view_id" ref="cetak"/>
      <field name="target">current</field>
  </record>
    
    <template id="cetak">

    </template>

    <template id="project_kanban_template" name="Project Kanban">
      <t t-name="project.kanban">
          <div class="project-kanban">
              <p>
                Lorem, ipsum.
              </p>
              <h2>Project: <t t-esc="record.name"/></h2>
              <p>Description: <t t-esc="record.description"/></p>
          </div>
      </t>
  </template>
    <!-- explicit list view definition -->


    
     <record model="ir.ui.view" id="barcodescaner.kanban">
      <field name="name">barcodescaner list</field>
      <field name="model">barcodescaner.barcodescaner</field>
      <field name="arch" type="xml">
        <kanban>
          <templates>
              <t t-name="kanban-box">
                  <div class="oe_kanban_content">
                    <h2>Content</h2>
                      <div class="oe_kanban_global_click">
                        <button>Help</button>
                        <h1>Hello word</h1>  
                  </div>
                  </div>
              </t>
          </templates>
      </kanban>
      </field>
    </record> 
    <record model="ir.ui.view" id="barcodescaner.list">
      <field name="name">barcodescaner list</field>
      <field name="model">barcodescaner.barcodescaner</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="value"/>
          <field name="value2"/>
        </tree>
       
      </field>
      
    </record>

    <record model="ir.ui.view" id="barcodescaner.form">
      <field name="name">barcodescaner Form</field>
      <field name="model">barcodescaner.barcodescaner</field>
      <field name="arch" type="xml">
        <form>
         
          <sheet>
            <group>
              <field name="name"/>
              <field name="value"/>
              <field name="value2"/>
            </group>
            <div>
                  <h1>Hello</h1>  
            </div>
           
          </sheet>
          
        </form>
        
       
      </field>
      
    </record>
    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="barcodescaner.action_window">
      <field name="name">barcodescaner window</field>
      <field name="res_model">barcodescaner.barcodescaner</field>
      <field name="view_mode">tree,form,kanban</field>
    </record>

    <record model="ir.actions.act_window" id="barcodescaner.action_window_tree">
      <field name="name">barcodescaner window</field>
      <field name="res_model">barcodescaner.barcodescaner</field>
      <field name="view_mode">tree</field>
    </record>

    <record model="ir.actions.act_window" id="barcodescaner.action_window_form">
      <field name="name">barcodescaner window</field>
      <field name="res_model">barcodescaner.barcodescaner</field>
      <field name="view_mode">form</field>
    </record>
    <record model="ir.actions.act_window" id="barcodescaner.action_window_kanban">
      <field name="name">barcodescaner window</field>
      <field name="res_model">barcodescaner.barcodescaner</field>
      <field name="view_mode">kanban</field>
    </record>
    <record model="ir.actions.act_window" id="barcodescaner.action_window_qweb">
      <field name="name">barcodescaner window</field>
      <field name="res_model">barcodescaner.barcodescaner</field>
      <field name="view_mode">QWeb</field>
      <field name="view_id" ref="cetak"/>
      <field name="target">current</field>
    </record>
    

    <!-- server action to the one above -->

    <record model="ir.actions.server" id="barcodescaner.action_server">
      <field name="name">barcodescaner server</field>
      <field name="model_id" ref="model_barcodescaner_barcodescaner"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_url",
          "url": "barcodescaner", 
          "target":"self"
        }
      </field>
    </record>

    <record model="ir.actions.server" id="barcodescaner.action_server_landing">
      <field name="name">barcodescaner server</field>
      <field name="model_id" ref="model_barcodescaner_barcodescaner"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_url",
          "url": "barcodescaner/landing", 
          "target":"self"
        }
      </field>
    </record>

    
    <!-- Top menu item -->

    <menuitem name="barcodescaner" id="barcodescaner.menu_root"/>

    <!-- menu categories -->

    <menuitem name="View" id="barcodescaner.menu_1" parent="barcodescaner.menu_root"/>
    <menuitem name="Action Server" id="barcodescaner.menu_2" parent="barcodescaner.menu_root"/>
    <menuitem name="QWEB" id="barcodescaner.menu_3" parent="barcodescaner.menu_root"/>

    <!-- actions -->

    <menuitem name="List" id="barcodescaner.menu_1_list" parent="barcodescaner.menu_1"
              action="barcodescaner.action_window"/>
              <menuitem name="List" id="barcodescaner.menu_11_list" parent="barcodescaner.menu_1"
              action="barcodescaner.action_window_tree"/>          

    <menuitem name="Form" id="barcodescaner.menu_2_list" parent="barcodescaner.menu_1"
              action="barcodescaner.action_window_form"/>          
    <menuitem name="Kanban" id="barcodescaner.menu_3_list" parent="barcodescaner.menu_1"
              action="barcodescaner.action_window_kanban"/>
    <menuitem name="QWEB" id="barcodescaner.menu_4_list" parent="barcodescaner.menu_1"
              action="barcodescaner.action_window_qweb"/>
    <menuitem name="Report" id="barcodescaner.menu_5_list" parent="barcodescaner.menu_1"
              action="barcodescaner.action_window_qweb"/> 
    <menuitem name="Calendar" id="barcodescaner.menu_6_list" parent="barcodescaner.menu_1"
              action="barcodescaner.action_window_qweb"/>
    <menuitem name="Graph" id="barcodescaner.menu_7_list" parent="barcodescaner.menu_1"
              action="barcodescaner.action_window_qweb"/> 
    <menuitem name="Grant" id="barcodescaner.menu_8_list" parent="barcodescaner.menu_1"
              action="barcodescaner.action_window_qweb"/>
    <menuitem name="Search" id="barcodescaner.menu_9_list" parent="barcodescaner.menu_1"
              action="barcodescaner.action_window_qweb"/>
    <menuitem name="Activi" id="barcodescaner.menu_10_list" parent="barcodescaner.menu_1"
              action="barcodescaner.action_window_qweb"/>                                                            

    <menuitem name="Server to list" id="barcodescaner" parent="barcodescaner.menu_2"
              action="barcodescaner.action_server"/>
    <menuitem name="Landing Page" id="barcodescanerlanding" parent="barcodescaner.menu_2"
              action="barcodescaner.action_server_landing"/>
          

  </data>
</odoo>